
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  email         String        @id 
  name          String
  subscription  Subscription?
}

model Subscription {
  id            Int           @id @default(autoincrement())
  userId        String        @unique
  user          User          @relation(fields: [userId], references: [email])
}
