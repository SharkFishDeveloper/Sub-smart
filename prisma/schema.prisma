
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String     @id @default(cuid())
  email     String     @unique
  name      String?
  reminders Reminder[]
}

model Reminder {
  id     Int    @id @default(autoincrement())
  name   String
  dates  String[]
  userId String
  user   User   @relation(fields: [userId], references: [email])
}
